<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<title>webapp</title>
	</head>
	<body>
		<div class="index-layout" id="index-page">
			<!-- 头部 -->
			<div class="header-layout">
				{{include './components/header/header.art'}}
			</div>

			<!-- 幻灯片 -->
			<div class="slider-layout" id="index-slider">
				{{include '../../components/loading/loading.art'}}
			</div>

			<!-- 导航 -->
			<div class="nav-layout" id="index-nav">
				{{include '../../components/loading/loading.art'}}
			</div>

			<!-- 商品区 -->
			<div class="product-layout" id="index-product">
				{{include './components/product/product.art'}}
			</div>

			<!-- 标签栏 -->
			<div class="tabbar-layout">
				<% include('../../components/tabbar/tabbar.art', {active:'index'}) %>
			</div>

			<!-- 返回顶部按钮 -->
			<div class="backtop-layout">
				{{include '../../components/backtop/backtop.art'}}
			</div>
		</div>
	</body>
</html>
